<!DOCTYPE html>
<html>
  <head>
    <title>LoudCloud</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= favicon_link_tag asset_path("cloud.png") %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
   <script type="text/javascript">
    window.cloud = "<%= image_url('cloud.png') %>";
    window.play = "<%= image_url('play.png') %>";
    window.pause = "<%= image_url('pause.png') %>";
    window.forward = "<%= image_url('forward.png') %>";
    window.rewind = "<%= image_url('rewind.png') %>";
    window.repeat = "<%= image_url('repeat.png') %>";
    window.shuffle = "<%= image_url('shuffle.png') %>";
   </script>
   


  </head>

  <body>
    <%= yield %>
  </body>
</html>
